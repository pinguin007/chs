body, html, #root, .main-wrapper {
  min-height: 100vh;
  margin: 0;
  padding: 0;
  font-family: 'Inter', 'Segoe UI', Arial, sans-serif;
  background: linear-gradient(to bottom, #f7faff 0%, #e3eaff 50%, #d4d9f5 100%);
  color: #1a2340;
  position: relative;
  overflow-x: hidden;
}

.main-wrapper::before {
  content: '';
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-image: 
    radial-gradient(1px 1px at 20px 30px, rgba(99,102,241,0.15), rgba(0,0,0,0)),
    radial-gradient(1px 1px at 40px 70px, rgba(99,102,241,0.15), rgba(0,0,0,0)),
    radial-gradient(1px 1px at 50px 160px, rgba(99,102,241,0.15), rgba(0,0,0,0)),
    radial-gradient(1px 1px at 90px 40px, rgba(99,102,241,0.15), rgba(0,0,0,0));
  background-repeat: repeat;
  background-size: 200px 200px;
  opacity: 0.4;
  z-index: 0;
  pointer-events: none;
}

.navbar {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 1.5rem 2rem;
  background: rgba(255,255,255,0.7);
  backdrop-filter: blur(12px);
  -webkit-backdrop-filter: blur(12px);
  border: 1.5px solid rgba(255,255,255,0.3);
  border-radius: 24px;
  margin: 1rem 2rem;
  position: relative;
  z-index: 1000;
}

.nav-menu {
  display: flex;
  align-items: center;
  gap: 2rem;
}

.nav-links {
  display: flex;
  gap: 2rem;
  list-style: none;
  margin: 0;
  padding: 0;
}

.nav-links li a {
  color: #1a2340;
  text-decoration: none;
  font-weight: 500;
  transition: color 0.3s ease;
}

.nav-links li a:hover {
  color: #3b82f6;
}

.logo {
  font-weight: bold;
  font-size: 2rem;
  letter-spacing: 2px;
  color: #3b82f6;
  text-shadow: 0 2px 8px rgba(59,130,246,0.15);
}

.nav-actions .btn.small {
  padding: 0.5rem 1.2rem;
  font-size: 1rem;
}

.btn {
  background: #3b82f6;
  color: #fff;
  border: none;
  border-radius: 8px;
  padding: 0.8rem 2rem;
  margin-right: 1rem;
  font-size: 1.1rem;
  cursor: pointer;
  transition: background 0.2s;
}

.btn.outline {
  background: #fff;
  color: #3b82f6;
  border: 1.5px solid #3b82f6;
}

.hero {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 6rem 4rem;
  background: linear-gradient(120deg, rgba(224,231,255,0.8) 0%, rgba(240,249,255,0.8) 100%);
  border-radius: 24px;
  box-shadow: 0 8px 32px 0 rgba(31,38,135,0.10);
  margin: 2rem;
  position: relative;
  overflow: hidden;
  min-height: 80vh;
  transition: all 0.3s ease;
}

.hero::before {
  content: '';
  position: absolute;
  top: -120px;
  right: -120px;
  width: 400px;
  height: 400px;
  background: radial-gradient(circle, #fff 0%, #3b82f6 100%);
  opacity: 0.15;
  filter: blur(60px);
  z-index: 0;
}

.hero::after {
  content: '';
  position: absolute;
  top: 0;
  right: 0;
  width: 100%;
  height: 100%;
  background: radial-gradient(circle at 70% 30%, rgba(99,102,241,0.1) 0%, transparent 70%);
  pointer-events: none;
}

.hero-content {
  max-width: 650px;
  position: relative;
  z-index: 1;
}

.hero-content h1 {
  font-size: 4.5rem;
  line-height: 1.1;
  margin-bottom: 1.5rem;
  background: linear-gradient(90deg, #3b82f6 30%, #6366f1 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}

.hero-content p {
  font-size: 1.5rem;
  line-height: 1.6;
  margin-bottom: 3rem;
  color: #334155;
  max-width: 540px;
}

.hero-image {
  width: 500px;
  height: 400px;
  background: rgba(255,255,255,0.25);
  border-radius: 32px;
  box-shadow: 0 8px 32px 0 rgba(31,38,135,0.10);
  backdrop-filter: blur(8px);
  -webkit-backdrop-filter: blur(8px);
  border: 1.5px solid rgba(255,255,255,0.18);
  position: relative;
  z-index: 1;
  transform: perspective(1000px) rotateY(-15deg);
  transition: transform 0.3s ease;
}

.hero-image:hover {
  transform: perspective(1000px) rotateY(-5deg) translateY(-10px);
}

.hero-actions {
  display: flex;
  gap: 1rem;
}

.hero-actions .btn {
  padding: 1rem 2.5rem;
  font-size: 1.2rem;
  font-weight: 500;
}

.cards-section {
  padding: 8rem 4rem;
  background: linear-gradient(135deg, rgba(30, 58, 138, 0.1) 0%, rgba(29, 78, 216, 0.05) 100%);
  position: relative;
  overflow: hidden;
}

.cards-section h2 {
  font-size: 2.5rem;
  margin-bottom: 2rem;
  background: linear-gradient(90deg, #1e40af 0%, #3b82f6 50%, #60a5fa 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}

.section-intro {
  max-width: 800px;
  margin: 0 auto 4rem auto;
  text-align: center;
}

.section-intro p {
  font-size: 1.25rem;
  line-height: 1.7;
  opacity: 0.9;
}

.cards-section .section-intro p {
  color: #1e40af;
}

.trusted-by .section-intro p {
  color: #1e3a8a;
}

.content-speed .section-intro p {
  color: #1e3a8a;
}

.testimonials .section-intro p {
  color: #1e3a8a;
}

.solutions-grid {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: 2rem;
  max-width: 1200px;
  margin: 0 auto;
}

.solution-card {
  background: rgba(255, 255, 255, 0.15);
  border-radius: 20px;
  overflow: hidden;
  backdrop-filter: blur(8px);
  -webkit-backdrop-filter: blur(8px);
  border: 1px solid rgba(147, 197, 253, 0.3);
  transition: all 0.3s ease;
  box-shadow: 0 4px 20px rgba(30, 58, 138, 0.1);
}

.solution-visual {
  width: 100%;
  height: 200px;
  background: linear-gradient(135deg, rgba(37, 99, 235, 0.1) 0%, rgba(59, 130, 246, 0.2) 100%);
  position: relative;
  overflow: hidden;
}

.solution-content {
  padding: 2rem;
  text-align: left;
}

.solution-content h3 {
  font-size: 1.5rem;
  color: #1e40af;
  margin-bottom: 1rem;
}

.solution-content p {
  color: #334155;
  line-height: 1.6;
}

.solution-card:hover {
  transform: translateY(-10px);
  box-shadow: 0 20px 40px rgba(30, 58, 138, 0.2),
              0 0 20px rgba(147, 197, 253, 0.2);
  border-color: rgba(147, 197, 253, 0.5);
}

.trusted-by {
  padding: 6rem 4rem;
  background: linear-gradient(135deg, rgba(219, 234, 254, 0.1) 0%, rgba(191, 219, 254, 0.05) 100%);
  backdrop-filter: blur(10px);
  -webkit-backdrop-filter: blur(10px);
  text-align: center;
}

.trusted-by h2 {
  font-size: 2.5rem;
  margin-bottom: 4rem;
  background: linear-gradient(90deg, #1e40af 0%, #3b82f6 50%, #60a5fa 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}

.partners-showcase {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: 3rem;
  max-width: 1200px;
  margin: 0 auto;
}

.partner-category {
  padding: 2rem;
  background: rgba(255, 255, 255, 0.2);
  border-radius: 16px;
  backdrop-filter: blur(8px);
  -webkit-backdrop-filter: blur(8px);
  border: 1px solid rgba(191, 219, 254, 0.3);
  box-shadow: 0 4px 15px rgba(59, 130, 246, 0.08);
  transition: all 0.3s ease;
  display: flex;
  flex-direction: column;
  align-items: center;
  text-align: center;
}

.partner-icon {
  width: 64px;
  height: 64px;
  background: linear-gradient(135deg, rgba(96, 165, 250, 0.2) 0%, rgba(59, 130, 246, 0.1) 100%);
  border-radius: 50%;
  margin-bottom: 1.5rem;
  display: flex;
  align-items: center;
  justify-content: center;
  transition: all 0.3s ease;
  box-shadow: 0 4px 12px rgba(59, 130, 246, 0.1);
}

.partner-category h3 {
  font-size: 1.25rem;
  color: #1e40af;
  margin-bottom: 1rem;
}

.partner-category p {
  color: #334155;
  line-height: 1.5;
  font-size: 0.95rem;
}

.partner-category:hover {
  transform: translateY(-5px);
  box-shadow: 0 15px 30px rgba(59, 130, 246, 0.15),
              0 0 15px rgba(191, 219, 254, 0.3);
  border-color: rgba(191, 219, 254, 0.5);
}

.partner-category:hover .partner-icon {
  background: linear-gradient(135deg, rgba(96, 165, 250, 0.3) 0%, rgba(59, 130, 246, 0.2) 100%);
  transform: scale(1.1);
}

.content-speed {
  padding: 6rem 4rem;
  background: linear-gradient(135deg, rgba(59, 130, 246, 0.08) 0%, rgba(37, 99, 235, 0.05) 100%);
  text-align: center;
}

.content-speed h2 {
  font-size: 2.5rem;
  margin-bottom: 3rem;
  background: linear-gradient(90deg, #1e40af 0%, #3b82f6 50%, #60a5fa 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}

.features-row {
  display: flex;
  justify-content: center;
  gap: 3rem;
  padding: 0 4rem;
}

.feature-card {
  background: rgba(255, 255, 255, 0.18);
  border-radius: 24px;
  box-shadow: 0 8px 30px rgba(59, 130, 246, 0.1);
  padding: 3rem;
  font-size: 1.2rem;
  min-width: 280px;
  min-height: 220px;
  text-align: center;
  backdrop-filter: blur(8px);
  -webkit-backdrop-filter: blur(8px);
  border: 1px solid rgba(59, 130, 246, 0.25);
  transition: all 0.3s ease;
  color: #1e40af;
}

.feature-card h3 {
  font-size: 1.5rem;
  margin-bottom: 1rem;
  color: #1e40af;
}

.feature-card p {
  color: #334155;
  line-height: 1.6;
  margin-bottom: 1.5rem;
}

.feature-visual {
  width: 100%;
  height: 160px;
  margin-top: 1.5rem;
  background: rgba(255, 255, 255, 0.15);
  border-radius: 16px;
  backdrop-filter: blur(4px);
  -webkit-backdrop-filter: blur(4px);
  border: 1px solid rgba(255, 255, 255, 0.18);
  transition: transform 0.3s ease;
}

.feature-card:hover {
  transform: translateY(-10px);
  box-shadow: 0 20px 40px rgba(59, 130, 246, 0.15),
              0 0 20px rgba(147, 197, 253, 0.2);
  border-color: rgba(59, 130, 246, 0.4);
}

.testimonial {
  padding: 2rem;
  background: rgba(255, 255, 255, 0.1);
  border-radius: 24px;
  backdrop-filter: blur(8px);
  -webkit-backdrop-filter: blur(8px);
  border: 1px solid rgba(255, 255, 255, 0.2);
  margin: 1rem;
  flex: 1;
  position: relative;
}

.testimonial-visual {
  width: 100%;
  height: 160px;
  margin-top: 1.5rem;
  background: rgba(255, 255, 255, 0.15);
  border-radius: 16px;
  backdrop-filter: blur(4px);
  -webkit-backdrop-filter: blur(4px);
  border: 1px solid rgba(255, 255, 255, 0.18);
}

.testimonial:hover {
  transform: translateY(-10px);
  box-shadow: 0 15px 45px rgba(59,130,246,0.2);
}

.universe {
  padding: 8rem 4rem;
  background: linear-gradient(to bottom, #0f172a, #1e1b4b);
  color: white;
  position: relative;
  overflow: hidden;
  min-height: 80vh;
}

.universe::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-image: 
    radial-gradient(2px 2px at 20px 30px, #ffffff, rgba(0,0,0,0)),
    radial-gradient(2px 2px at 40px 70px, #ffffff, rgba(0,0,0,0)),
    radial-gradient(2px 2px at 50px 160px, #ffffff, rgba(0,0,0,0)),
    radial-gradient(2px 2px at 90px 40px, #ffffff, rgba(0,0,0,0)),
    radial-gradient(2px 2px at 130px 80px, #ffffff, rgba(0,0,0,0)),
    radial-gradient(2px 2px at 160px 120px, #ffffff, rgba(0,0,0,0));
  background-repeat: repeat;
  background-size: 200px 200px;
  animation: twinkle 4s infinite;
  opacity: 0.5;
}

.universe::after {
  content: '';
  position: absolute;
  top: 0;
  left: 100px;
  right: 0;
  bottom: 0;
  background-image: 
    radial-gradient(2px 2px at 100px 150px, #ffffff, rgba(0,0,0,0)),
    radial-gradient(2px 2px at 200px 50px, #ffffff, rgba(0,0,0,0)),
    radial-gradient(2px 2px at 300px 160px, #ffffff, rgba(0,0,0,0)),
    radial-gradient(2px 2px at 400px 240px, #ffffff, rgba(0,0,0,0)),
    radial-gradient(2px 2px at 500px 80px, #ffffff, rgba(0,0,0,0)),
    radial-gradient(2px 2px at 50px 50px, #ffffff, rgba(0,0,0,0));
  background-repeat: repeat;
  background-size: 200px 200px;
  animation: twinkle 6s infinite;
  opacity: 0.5;
}

.shooting-star {
  position: absolute;
  width: 100px;
  height: 2px;
  background: linear-gradient(90deg, #ffffff, transparent);
  animation: shooting 4s linear infinite;
}

.shooting-star:nth-child(1) {
  top: 20%;
  left: 0;
  animation-delay: 0s;
}

.shooting-star:nth-child(2) {
  top: 40%;
  right: 0;
  animation-delay: 2s;
}

@keyframes shooting {
  0% {
    transform: translateX(-100px) rotate(45deg);
    opacity: 1;
  }
  100% {
    transform: translateX(calc(100vw + 100px)) rotate(45deg);
    opacity: 0;
  }
}

@keyframes twinkle {
  0% { opacity: 0; }
  50% { opacity: 1; }
  100% { opacity: 0; }
}

.universe-content {
  position: relative;
  z-index: 2;
  max-width: 800px;
  margin: 0 auto;
  text-align: center;
}

.universe-content h2 {
  font-size: 3rem;
  background: linear-gradient(135deg, #c7d2fe 0%, #818cf8 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  margin-bottom: 2rem;
  text-shadow: 0 0 20px rgba(99,102,241,0.5);
}

.universe-content p {
  font-size: 1.25rem;
  line-height: 1.7;
  color: #c7d2fe;
  margin-bottom: 3rem;
}

.impact-stats {
  display: flex;
  justify-content: space-around;
  margin: 4rem 0;
  gap: 2rem;
}

.stat-item {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 2rem;
  background: rgba(255, 255, 255, 0.05);
  border-radius: 20px;
  backdrop-filter: blur(10px);
  border: 1px solid rgba(255, 255, 255, 0.1);
  min-width: 200px;
  transform: translateY(0);
  transition: all 0.3s ease;
}

.stat-item:hover {
  transform: translateY(-10px);
  background: rgba(255, 255, 255, 0.1);
  border-color: rgba(255, 255, 255, 0.2);
}

.stat-number {
  font-size: 3rem;
  font-weight: bold;
  background: linear-gradient(135deg, #c7d2fe 0%, #818cf8 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  margin-bottom: 0.5rem;
}

.stat-label {
  color: #c7d2fe;
  font-size: 1.1rem;
}

.charity-section {
  margin-top: 8rem;
  display: grid;
  grid-template-columns: 1.2fr 0.8fr;
  gap: 4rem;
  max-width: 1200px;
  margin: 8rem auto 0;
  position: relative;
  z-index: 2;
}

.charity-content h3 {
  font-size: 2.5rem;
  background: linear-gradient(135deg, #c7d2fe 0%, #818cf8 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  margin-bottom: 2rem;
  text-shadow: 0 0 20px rgba(99,102,241,0.3);
}

.charity-content > p {
  font-size: 1.2rem;
  line-height: 1.7;
  color: #c7d2fe;
  margin-bottom: 4rem;
}

.charity-pillars {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: 2rem;
}

.pillar {
  background: rgba(255, 255, 255, 0.05);
  border-radius: 20px;
  padding: 2rem;
  backdrop-filter: blur(10px);
  border: 1px solid rgba(255, 255, 255, 0.1);
  transition: all 0.3s ease;
}

.pillar:hover {
  transform: translateY(-10px);
  background: rgba(255, 255, 255, 0.1);
  border-color: rgba(255, 255, 255, 0.2);
}

.pillar-icon {
  width: 60px;
  height: 60px;
  border-radius: 50%;
  margin-bottom: 1.5rem;
  background: linear-gradient(135deg, rgba(199, 210, 254, 0.2), rgba(129, 140, 248, 0.2));
  display: flex;
  align-items: center;
  justify-content: center;
}

.pillar h4 {
  font-size: 1.25rem;
  color: #c7d2fe;
  margin-bottom: 1rem;
}

.pillar p {
  color: #a5b4fc;
  line-height: 1.6;
}

.charity-visual {
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
}

.impact-globe {
  width: 400px;
  height: 400px;
  background: linear-gradient(135deg, rgba(199, 210, 254, 0.1), rgba(129, 140, 248, 0.1));
  border-radius: 50%;
  position: relative;
  overflow: hidden;
  backdrop-filter: blur(10px);
  border: 1px solid rgba(255, 255, 255, 0.1);
  box-shadow: 
    0 0 60px rgba(129, 140, 248, 0.3),
    0 0 100px rgba(199, 210, 254, 0.2);
  animation: float 8s ease-in-out infinite;
}

.impact-globe::before {
  content: '';
  position: absolute;
  top: -50%;
  left: -50%;
  width: 200%;
  height: 200%;
  background: linear-gradient(
    45deg,
    transparent 0%,
    rgba(129, 140, 248, 0.1) 50%,
    transparent 100%
  );
  transform: rotate(45deg);
  animation: rotate 12s linear infinite;
}

@keyframes rotate {
  from { transform: rotate(0deg); }
  to { transform: rotate(360deg); }
}

@keyframes float {
  0% { transform: translateY(0px); }
  50% { transform: translateY(-20px); }
  100% { transform: translateY(0px); }
}

@media (max-width: 1024px) {
  .charity-section {
    grid-template-columns: 1fr;
    gap: 4rem;
  }
  .charity-pillars {
    grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
  }
  .impact-globe {
    width: 300px;
    height: 300px;
  }
}

@media (max-width: 768px) {
  .impact-stats {
    flex-direction: column;
    align-items: center;
  }
  .stat-item {
    width: 100%;
    max-width: 300px;
  }
}

.footer {
  background: linear-gradient(135deg, rgba(30, 58, 138, 0.95) 0%, rgba(29, 78, 216, 0.95) 100%);
  padding: 6rem 4rem 2rem;
  color: white;
}

.footer-content {
  max-width: 1200px;
  margin: 0 auto;
  display: grid;
  grid-template-columns: 1fr 2fr;
  gap: 4rem;
  margin-bottom: 4rem;
}

.footer-brand {
  display: flex;
  flex-direction: column;
  gap: 1.5rem;
}

.footer-logo {
  font-size: 2rem;
  font-weight: bold;
  background: linear-gradient(90deg, #60a5fa 0%, #93c5fd 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}

.social-links {
  display: flex;
  gap: 1.5rem;
}

.social-links span {
  cursor: pointer;
  transition: color 0.3s ease;
}

.social-links span:hover {
  color: #93c5fd;
}

.footer-links {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: 3rem;
}

.link-group h4 {
  color: #93c5fd;
  margin-bottom: 1.5rem;
}

.link-group ul {
  list-style: none;
  padding: 0;
}

.link-group li {
  margin-bottom: 0.8rem;
  cursor: pointer;
  transition: color 0.3s ease;
}

.link-group li:hover {
  color: #93c5fd;
}

.footer-bottom {
  max-width: 1200px;
  margin: 0 auto;
  padding-top: 2rem;
  border-top: 1px solid rgba(147, 197, 253, 0.2);
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #93c5fd;
}

.legal-links {
  display: flex;
  gap: 2rem;
}

.legal-links span {
  cursor: pointer;
  transition: color 0.3s ease;
}

.legal-links span:hover {
  color: white;
}

/* New glassy sections */
.roadmap-section, .faq-section, .cta-section, .newsletter-section, .team-section {
  background: rgba(255,255,255,0.22);
  border-radius: 24px;
  box-shadow: 0 8px 32px 0 rgba(31,38,135,0.10);
  backdrop-filter: blur(10px);
  -webkit-backdrop-filter: blur(10px);
  border: 1.5px solid rgba(255,255,255,0.18);
  margin: 3rem auto;
  padding: 3rem 2rem;
  max-width: 1100px;
}

.roadmap-timeline {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  gap: 2rem;
  margin-top: 2rem;
}

.roadmap-step {
  flex: 1;
  background: rgba(59,130,246,0.08);
  border-radius: 14px;
  padding: 1.5rem 1rem;
  margin: 0 0.5rem;
  box-shadow: 0 2px 8px rgba(59,130,246,0.08);
}

.faq-list {
  margin-top: 2rem;
}

.faq-item {
  margin-bottom: 1.5rem;
}

.faq-q {
  font-weight: bold;
  color: #3b82f6;
}

.faq-a {
  color: #334155;
}

.cta-section {
  text-align: center;
  background: linear-gradient(120deg, #3b82f6 60%, #6366f1 100%);
  color: #fff;
  border: none;
  box-shadow: 0 8px 32px 0 rgba(59,130,246,0.15);
}

.cta-section .btn {
  background: #fff;
  color: #3b82f6;
  border: 1.5px solid #fff;
  margin-top: 1.5rem;
}

.newsletter-section input[type="email"] {
  padding: 0.8rem 1.2rem;
  border-radius: 8px;
  border: 1.5px solid #3b82f6;
  margin-right: 1rem;
  font-size: 1.1rem;
}

.newsletter-section .btn {
  padding: 0.8rem 2rem;
  font-size: 1.1rem;
}

.team-grid {
  display: flex;
  gap: 2rem;
  justify-content: center;
  margin-top: 2rem;
}

.team-member {
