/* Testimonials Section - Modern Portfolio Showcase */
.testimonials {
  padding: 8rem 4rem;
  background: linear-gradient(135deg, rgba(30, 64, 175, 0.08) 0%, rgba(29, 78, 216, 0.05) 100%);
  backdrop-filter: blur(10px);
  -webkit-backdrop-filter: blur(10px);
}

.testimonials h2 {
  font-size: 2.5rem;
  text-align: center;
  margin-bottom: 4rem;
  background: linear-gradient(90deg, #1e40af 0%, #3b82f6 50%, #60a5fa 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}

.testimonials-showcase {
  max-width: 1200px;
  margin: 0 auto;
  display: flex;
  flex-direction: column;
  gap: 3rem;
}

.testimonial-featured {
  display: grid;
  grid-template-columns: 1.2fr 1fr;
  gap: 2rem;
  padding: 3rem;
  background: rgba(255, 255, 255, 0.15);
  border-radius: 24px;
  backdrop-filter: blur(8px);
  -webkit-backdrop-filter: blur(8px);
  border: 1px solid rgba(96, 165, 250, 0.3);
  box-shadow: 0 8px 30px rgba(30, 64, 175, 0.1);
  transition: all 0.3s ease;
}

.testimonial-featured:hover {
  transform: translateY(-10px);
  box-shadow: 0 25px 50px rgba(30, 64, 175, 0.15),
              0 0 20px rgba(96, 165, 250, 0.2);
  border-color: rgba(96, 165, 250, 0.5);
}

.testimonial-header {
  display: flex;
  align-items: center;
  gap: 1rem;
  margin-bottom: 1.5rem;
}

.testimonial-header h3 {
  font-size: 2rem;
  color: #1e40af;
}

.testimonial-badge {
  padding: 0.5rem 1rem;
  background: linear-gradient(135deg, rgba(37, 99, 235, 0.1) 0%, rgba(59, 130, 246, 0.2) 100%);
  border-radius: 20px;
  border: 1px solid rgba(96, 165, 250, 0.3);
  color: #1e40af;
  font-size: 0.9rem;
  font-weight: 500;
}

.testimonial-stats {
  display: flex;
  gap: 2rem;
  margin-top: 2rem;
  padding-top: 2rem;
  border-top: 1px solid rgba(255, 255, 255, 0.1);
}

.stat {
  display: flex;
  flex-direction: column;
  gap: 0.5rem;
}

.stat-number {
  font-size: 2rem;
  font-weight: 600;
  color: #1e40af;
}

.stat-label {
  font-size: 0.9rem;
  color: #334155;
}

.testimonial-visual.featured {
  height: 100%;
  border-radius: 16px;
  background: rgba(255, 255, 255, 0.15);
}

.testimonials-grid {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: 2rem;
}

.testimonial-card {
  background: rgba(255, 255, 255, 0.12);
  border-radius: 20px;
  overflow: hidden;
  backdrop-filter: blur(8px);
  -webkit-backdrop-filter: blur(8px);
  border: 1px solid rgba(96, 165, 250, 0.2);
  transition: all 0.3s ease;
  box-shadow: 0 8px 25px rgba(30, 64, 175, 0.08);
}

.testimonial-card:hover {
  transform: translateY(-10px);
  box-shadow: 0 20px 40px rgba(30, 64, 175, 0.12),
              0 0 20px rgba(96, 165, 250, 0.15);
  border-color: rgba(96, 165, 250, 0.4);
}

.testimonial-card .testimonial-visual {
  width: 100%;
  height: 180px;
  background: rgba(255, 255, 255, 0.15);
}

.testimonial-card .testimonial-content {
  padding: 2rem;
}

.testimonial-card h4 {
  font-size: 1.5rem;
  color: #1e40af;
  margin-bottom: 1rem;
}

.testimonial-card p {
  color: #334155;
  line-height: 1.6;
  margin-bottom: 1.5rem;
}

.testimonial-tag {
  display: inline-block;
  padding: 0.5rem 1rem;
  background: linear-gradient(135deg, rgba(37, 99, 235, 0.15) 0%, rgba(59, 130, 246, 0.25) 100%);
  border-radius: 20px;
  border: 1px solid rgba(96, 165, 250, 0.3);
  color: #1e40af;
  font-size: 0.85rem;
  font-weight: 500;
}

/* Responsive styles for Testimonials section */
@media (max-width: 1200px) {
  .testimonials {
    padding: 6rem 3rem;
  }

  .testimonials-showcase {
    padding: 0 1rem;
  }

  .testimonial-featured {
    padding: 2.5rem;
  }

  .testimonials-grid {
    gap: 1.5rem;
  }
}

@media (max-width: 1024px) {
  .testimonial-featured {
    grid-template-columns: 1fr;
    padding: 2rem;
  }

  .testimonial-visual.featured {
    height: 300px;
    order: -1;
  }

  .testimonials-grid {
    grid-template-columns: repeat(2, 1fr);
  }

  .testimonial-header {
    flex-direction: column;
    align-items: flex-start;
    text-align: left;
  }

  .testimonial-stats {
    justify-content: flex-start;
  }
}

@media (max-width: 768px) {
  .testimonials {
    padding: 4rem 2rem;
  }

  .testimonials h2 {
    font-size: 2rem;
    margin-bottom: 3rem;
  }

  .testimonials-showcase {
    gap: 2rem;
  }

  .testimonial-featured {
    padding: 1.5rem;
  }

  .testimonial-header h3 {
    font-size: 1.75rem;
  }

  .testimonial-visual.featured {
    height: 250px;
  }

  .testimonials-grid {
    grid-template-columns: 1fr;
    max-width: 500px;
    margin: 0 auto;
  }

  .testimonial-card .testimonial-visual {
    height: 200px;
  }

  .testimonial-card .testimonial-content {
    padding: 1.5rem;
  }

  .testimonial-card h4 {
    font-size: 1.3rem;
  }

  .testimonial-stats {
    flex-direction: column;
    gap: 1rem;
  }

  .stat {
    text-align: left;
  }
}

@media (max-width: 480px) {
  .testimonials {
    padding: 3rem 1rem;
  }

  .testimonials h2 {
    font-size: 1.75rem;
    margin-bottom: 2rem;
  }

  .testimonial-featured {
    padding: 1.25rem;
  }

  .testimonial-header h3 {
    font-size: 1.5rem;
  }

  .testimonial-visual.featured {
    height: 200px;
  }

  .testimonial-card .testimonial-visual {
    height: 180px;
  }

  .testimonial-card .testimonial-content {
    padding: 1.25rem;
  }

  .testimonial-card h4 {
    font-size: 1.2rem;
  }

  .testimonial-badge {
    padding: 0.4rem 0.8rem;
    font-size: 0.8rem;
  }

  .testimonial-tag {
    padding: 0.4rem 0.8rem;
    font-size: 0.8rem;
  }
}

/* Roadmap Section */
.roadmap-section {
  padding: 8rem 4rem;
  background: linear-gradient(135deg, rgba(30, 64, 175, 0.05) 0%, rgba(29, 78, 216, 0.02) 100%);
}

.roadmap-container {
  max-width: 1200px;
  margin: 0 auto;
}

.roadmap-timeline {
  display: flex;
  flex-direction: column;
  gap: 4rem;
  position: relative;
}

.roadmap-timeline::before {
  content: '';
  position: absolute;
  left: 40px;
  top: 0;
  bottom: 0;
  width: 2px;
  background: linear-gradient(to bottom, rgba(59, 130, 246, 0.2), rgba(96, 165, 250, 0.1));
}

.roadmap-step {
  display: grid;
  grid-template-columns: 1fr 300px;
  gap: 3rem;
  padding-left: 6rem;
  position: relative;
}

.step-marker {
  position: absolute;
  left: -12px;
  width: 40px;
  height: 40px;
  background: linear-gradient(135deg, #3b82f6 0%, #60a5fa 100%);
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  color: white;
  font-weight: bold;
  box-shadow: 0 0 20px rgba(59, 130, 246, 0.3);
}

.step-content {
  background: rgba(255, 255, 255, 0.1);
  border-radius: 24px;
  padding: 2rem;
  backdrop-filter: blur(8px);
  -webkit-backdrop-filter: blur(8px);
  border: 1px solid rgba(96, 165, 250, 0.2);
  box-shadow: 0 8px 30px rgba(30, 64, 175, 0.08);
  transition: all 0.3s ease;
}

.step-content:hover {
  transform: translateY(-5px);
  box-shadow: 0 15px 40px rgba(30, 64, 175, 0.12);
  border-color: rgba(96, 165, 250, 0.4);
}

.step-details {
  margin-top: 1rem;
  padding-left: 1.5rem;
}

.step-details li {
  color: #334155;
  margin-bottom: 0.5rem;
  position: relative;
}

.step-details li::before {
  content: 'â€¢';
  color: #3b82f6;
  position: absolute;
  left: -1rem;
}

.step-visual {
  background: rgba(255, 255, 255, 0.15);
  border-radius: 16px;
  height: 100%;
  min-height: 200px;
}

/* Responsive styles for Roadmap section */
@media (max-width: 1200px) {
  .roadmap-section {
    padding: 6rem 3rem;
  }

  .roadmap-step {
    grid-template-columns: 1fr 250px;
    gap: 2rem;
  }

  .step-content {
    padding: 1.75rem;
  }
}

@media (max-width: 1024px) {
  .roadmap-section {
    padding: 5rem 2.5rem;
  }

  .roadmap-step {
    grid-template-columns: 1fr;
    padding-left: 4rem;
  }

  .step-visual {
    display: none;
  }

  .roadmap-timeline::before {
    left: 30px;
  }

  .step-marker {
    width: 32px;
    height: 32px;
    left: -10px;
    font-size: 0.9rem;
  }
}

@media (max-width: 768px) {
  .roadmap-section {
    padding: 4rem 2rem;
  }

  .roadmap-timeline {
    gap: 3rem;
  }

  .roadmap-step {
    padding-left: 3rem;
  }

  .step-content {
    padding: 1.5rem;
  }

  .step-content h4 {
    font-size: 1.2rem;
  }

  .step-content p {
    font-size: 1rem;
  }

  .step-details {
    padding-left: 1.25rem;
  }

  .step-details li {
    font-size: 0.95rem;
  }

  .roadmap-timeline::before {
    left: 24px;
  }

  .step-marker {
    width: 28px;
    height: 28px;
    left: -8px;
    font-size: 0.85rem;
  }
}

@media (max-width: 480px) {
  .roadmap-section {
    padding: 3rem 1rem;
  }

  .roadmap-timeline {
    gap: 2.5rem;
  }

  .roadmap-step {
    padding-left: 2.5rem;
  }

  .step-content {
    padding: 1.25rem;
  }

  .step-content h4 {
    font-size: 1.1rem;
  }

  .step-content p {
    font-size: 0.95rem;
  }

  .step-details {
    padding-left: 1rem;
  }

  .step-details li {
    font-size: 0.9rem;
    margin-bottom: 0.4rem;
  }

  .roadmap-timeline::before {
    left: 20px;
  }

  .step-marker {
    width: 24px;
    height: 24px;
    left: -6px;
    font-size: 0.8rem;
  }
}

